{
  "name": "react-address-select",
  "version": "0.0.1",
  "description": "A flexible React address autocomplete/select component with pluggable providers, locale/region biasing, and optional distance calculation.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladyslavPilkevych/react-address-select.git"
  },
  "homepage": "https://github.com/VladyslavPilkevych/react-address-select#readme",
  "bugs": {
    "url": "https://github.com/VladyslavPilkevych/react-address-select/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react-select": {
      "types": "./dist/react-select.d.ts",
      "import": "./dist/react-select.js",
      "require": "./dist/react-select.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "prepublishOnly": "pnpm run typecheck && pnpm run build",
    "demo": "pnpm run dev & pnpm -C demo dev",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-select": ">=5"
  },
  "peerDependenciesMeta": {
    "react-select": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "add": "^2.0.6",
    "jsdom": "^27.3.0",
    "pnpm": "^10.26.1",
    "react-select": "^5.10.2",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
